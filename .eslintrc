{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 2,
    "func-names": 0,
    "no-underscore-dangle": [0],
    "no-use-before-define": 0,
    "react/no-unused-prop-types": [2, { skipShapeProps: true }],
    "react/prefer-stateless-function": [0],
    "react/jsx-filename-extension": "off"
  }
}
